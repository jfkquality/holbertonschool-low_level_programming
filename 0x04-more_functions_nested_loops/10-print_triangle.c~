#include <stdio.h>
#include "holberton.h"

/**
 * print_square - print a square
 * @n: parameter
 * Return: void
 */

void print_triangle(int n)
{
	int i;
	int j;

	if (n <= 0)
		_putchar('\n');
	else
	{
		for (i = 0; i < n; i++)
		{
			for (j = n; j >= 0; j--)
			{
				_putchar(' ');
			}
			_putchar('#');
		};
	}
	_putchar('\n');
}
